{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "noEmit": true,
        "module": "commonjs",
        "target": "es6",
        "rootDir": "./",
        "typeRoots": [
            "./node_modules/@keycloak/keycloak-admin-client/lib/utils/auth",
            "./src/types"
        ]
    },
    "include": [
        "server/server.js",
        "server/**/*",
        "client/src/**/*"
    ],
    "exclude": [
        "test",
        "node_modules",
        "docs"
    ]
}
